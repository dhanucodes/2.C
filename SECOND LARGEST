#include<stdio.h>
void main()
{
    int n,i,first,second;
    printf("enter the number of elements:");
    scanf("%d",&n);
    printf("enter %d number",n);
    for(i=0;i<n;i++)
    {
      scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i++)
    {
        if(arr[i]>first)
        {
            second=first;
            first=arr[i];
        }
        else
            printf("the second largest number is %d\n",second);
    }
    return 0;

}
